# ==================== AI API Keys ====================
# Google Gemini API Key (OpenAI compatible)
# 获取地址: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_BASE_URL=https://generativelanguage.googleapis.com/v1beta/openai/
GEMINI_MODEL=gemini-2.5-flash
GEMINI_FALLBACK_MODEL=gemini-2.5-pro

# Azure OpenAI API Configuration (OpenAI compatible)
# 获取地址: https://portal.azure.com/
AZURE_OPENAI_API_ENDPOINT=https://your-resource-name.openai.azure.com/
AZURE_OPENAI_API_KEY=your_azure_openai_api_key_here
AZURE_OPENAI_API_VERSION=2024-12-01-preview
AZURE_OPENAI_DEPLOYMENT=your_azure_deployment_name
# 可选：直接指定完整 OpenAI 兼容 base_url
# AZURE_OPENAI_BASE_URL=https://your-resource-name.openai.azure.com/openai/deployments/your_azure_deployment_name

# ZhipuAI API Key (智谱AI, OpenAI compatible)
# 获取地址: https://open.bigmodel.cn/
ZHIPUAI_API_KEY=your_zhipuai_api_key_here
ZHIPU_BASE_URL=https://open.bigmodel.cn/api/paas/v4
ZHIPU_MODEL=glm-4.5-flash
ZHIPU_TRANSLATE_MODEL=glm-4.5-flash
ZHIPU_VISION_MODEL=glm-4.1v-thinking-flash

# Summary models (optional)
SUMMARY_MODEL=gemini-2.5-flash
SUMMARY_FALLBACK_MODEL=gemini-2.5-pro

# ==================== Telegram Bot ====================
# Telegram Bot Token
# 获取地址: https://t.me/BotFather
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# ==================== Database Configuration ====================
# MySQL Database Settings
MYSQL_HOST=localhost
MYSQL_USER=root
MYSQL_PASSWORD=your_mysql_password_here
MYSQL_DATABASE=telegram_bot_db

# ==================== Search API ====================
# SerpApi Key (Google Search)
# 获取地址: https://serpapi.com/
SERPAPI_API_KEY=your_serpapi_key_here

# ==================== Code Execution API ====================
# Judge0 API Key
# 获取地址: https://ce.judge0.com/
JUDGE0_API_KEY=your_judge0_api_key_here
